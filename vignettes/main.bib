
@article{lopes_sharp_2013,
	title = {A {Sharp} {Bound} on the {Computation}-{Accuracy} {Tradeoff} for {Majority} {Voting} {Ensembles}},
	url = {http://arxiv.org/abs/1303.0727},
	abstract = {When random forests are used for binary classification, an ensemble of \$t=1,2,{\textbackslash}dots\$ randomized classifiers is generated, and the predictions of the classifiers are aggregated by majority vote. Due to the randomness in the algorithm, there is a natural tradeoff between statistical performance and computational cost. On one hand, as \$t\$ increases, the (random) prediction error of the ensemble tends to decrease and stabilize. On the other hand, larger ensembles require greater computational cost for training and making new predictions. The present work offers a new approach for quantifying this tradeoff: Given a fixed training set \${\textbackslash}mathcal\{D\}\$, let the random variables \${\textbackslash}text\{Err\}\_\{t,0\}\$ and \${\textbackslash}text\{Err\}\_\{t,1\}\$ denote the class-wise prediction error rates of a randomly generated ensemble of size \$t\$. As \$t{\textbackslash}to{\textbackslash}infty\$, we provide a general bound on the "algorithmic variance", \${\textbackslash}text\{var\}({\textbackslash}text\{Err\}\_\{t,l\}{\textbar}{\textbackslash}mathcal\{D\}){\textbackslash}leq {\textbackslash}frac\{f\_l(1/2){\textasciicircum}2\}\{4t\}+o({\textbackslash}frac\{1\}\{t\})\$, where \$l{\textbackslash}in{\textbackslash}\{0,1{\textbackslash}\}\$, and \$f\_l\$ is a density function that arises from the ensemble method. Conceptually, this result is somewhat surprising, because \${\textbackslash}text\{var\}({\textbackslash}text\{Err\}\_\{t,l\}{\textbar}{\textbackslash}mathcal\{D\})\$ describes how \${\textbackslash}text\{Err\}\_\{t,l\}\$ varies over repeated runs of the algorithm, and yet, the formula leads to a method for bounding \${\textbackslash}text\{var\}({\textbackslash}text\{Err\}\_\{t,l\}{\textbar}{\textbackslash}mathcal\{D\})\$ with a single ensemble. The bound is also sharp in the sense that it is attained by an explicit family of randomized classifiers. With regard to the task of estimating \$f\_l(1/2)\$, the presence of the ensemble leads to a unique twist on the classical setup of non-parametric density estimation --- wherein the effects of sample size and computational cost are intertwined. In particular, we propose an estimator for \$f\_l(1/2)\$, and derive an upper bound on its MSE that matches "standard optimal non-parametric rates" when \$t\$ is sufficiently large.},
	urldate = {2018-08-16},
	journal = {arXiv:1303.0727 [cs, math, stat]},
	author = {Lopes, Miles E.},
	month = mar,
	year = {2013},
	note = {arXiv: 1303.0727},
	keywords = {Computer Science - Social and Information Networks, Mathematics - Probability, Mathematics - Statistics Theory, Statistics - Machine Learning},
	annote = {Comment: 28 pages. This paper extends the earlier work, "The Convergence Rate of Majority Vote under Exchangeability" from 2013},
	file = {arXiv\:1303.0727 PDF:C\:\\Users\\bfrank70\\Zotero\\storage\\WEMXM8B8\\Lopes - 2013 - A Sharp Bound on the Computation-Accuracy Tradeoff.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\bfrank70\\Zotero\\storage\\VTCZPXYE\\1303.html:text/html}
}

