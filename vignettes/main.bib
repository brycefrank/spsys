	year = {2001},
	doi = {10.1007/978-1-4613-0209-4_10},
	keywords = {Ecological Data, Explanatory Variable, Markov Chain Monte Carlo Method, Mathematical Geology, Spatial Autocorrelation},
	pages = {214--237},
