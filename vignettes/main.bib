@article{kralicek_simultaneous_2017,
	title = {Simultaneous estimation of above- and below-ground biomass in tropical forests of {Viet} {Nam}},
	volume = {390},
	issn = {0378-1127},
	url = {http://www.sciencedirect.com/science/article/pii/S0378112716307411},
	doi = {10.1016/j.foreco.2017.01.030},
	abstract = {For carbon accounting or for developing REDD+ (Reducing Emissions from Deforestation and forest Degradation) programs, allometric equations for estimating both above-ground biomass (AGB) and below-ground biomass (BGB) are useful. We developed systems of weighted nonlinear allometric equations to estimate total, above- and below-ground biomass for Dipterocarp forests (DF) and Evergreen broadleaf forests (EBLF) in the Central Highlands of Viet Nam, as well as for a dominant plant family (Dipterocarpaceae; Dip) in the DF. A total of 175 trees were destructively sampled for both AGB and BGB, with whole root extraction as the method of BGB sampling. Different equation forms for AGB and BGB incorporating diameter at breast height (D), tree height (H), wood density (WD) and crown area (CA) were evaluated. The best system of equations for the DF, Dipterocarpaceae in the DF, and EBLF was selected based on validation statistics of percent bias (PBias), mean absolute percentage error (MAPE), and root mean squared percent error (RMSPE). All three systems of equations developed in this study used D2×H×WD as a predictor for AGB and a simpler BGB equation form with either D2×H or D as the sole predictor variable. The addition of WD or CA to BGB equation forms did not substantially improve validation statistics over simpler forms. These allometric equations should contribute to advancing our understanding of carbon distribution of trees in these tropical ecosystems.},
	urldate = {2019-04-04},
	journal = {Forest Ecology and Management},
	author = {Kralicek, Karin and Huy, Bao and Poudel, Krishna P. and Temesgen, Hailemariam and Salas, Christian},
	month = apr,
	year = {2017},
	keywords = {Tropical forest, Additivity principle, Dipterocarp forest, Evergreen broadleaf forest, System of equations},
	pages = {147--156},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\bfrank70\\Zotero\\storage\\NY3VTKRX\\Kralicek et al. - 2017 - Simultaneous estimation of above- and below-ground.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\bfrank70\\Zotero\\storage\\GHW8SU8R\\S0378112716307411.html:text/html}
}

@article{poeplau_temporal_2011,
	title = {Temporal dynamics of soil organic carbon after land-use change in the temperate zone – carbon response functions as a model approach},
	volume = {17},
	copyright = {© 2011 Blackwell Publishing Ltd},
	issn = {1365-2486},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1365-2486.2011.02408.x},
	doi = {10.1111/j.1365-2486.2011.02408.x},
	abstract = {Land-use change (LUC) is a major driving factor for the balance of soil organic carbon (SOC) stocks and the global carbon cycle. The temporal dynamic of SOC after LUC is especially important in temperate systems with a long reaction time. On the basis of 95 compiled studies covering 322 sites in the temperate zone, carbon response functions (CRFs) were derived to model the temporal dynamic of SOC after five different LUC types (mean soil depth of 30±6 cm). Grassland establishment caused a long lasting carbon sink with a relative stock change of 128±23\% and afforestation on former cropland a sink of 116±54\%, 100 years after LUC (mean±95\% confidence interval). No new equilibrium was reached within 120 years. In contrast, there was no SOC sink following afforestation of grasslands and 75\% of all observations showed SOC losses, even after 100 years. Only in the forest floor, there was carbon accumulation of 0.38±0.04 Mg ha−1 yr−1 in afforestations adding up to 38±4 Mg ha−1 labile carbon after 100 years. Carbon loss after deforestation (−32±20\%) and grassland conversion to cropland (−36±5\%), was rapid with a new SOC equilibrium being reached after 23 and 17 years, respectively. The change rate of SOC increased with temperature and precipitation but decreased with soil depth and clay content. Subsoil SOC changes followed the trend of the topsoil SOC changes but were smaller (25±5\% of the total SOC changes) and with a high uncertainty due to a limited number of datasets. As a simple and robust model approach, the developed CRFs provide an easily applicable tool to estimate SOC stock changes after LUC to improve greenhouse gas reporting in the framework of UNFCCC.},
	language = {en},
	number = {7},
	urldate = {2019-04-04},
	journal = {Global Change Biology},
	author = {Poeplau, Christopher and Don, Axel and Vesterdal, Lars and Leifeld, Jens and Wesemael, Bas Van and Schumacher, Jens and Gensior, Andreas},
	year = {2011},
	keywords = {carbon response functions, land-use change, soil organic carbon, temperate zone, temporal dynamic},
	pages = {2415--2427},
	file = {Full Text PDF:C\:\\Users\\bfrank70\\Zotero\\storage\\5IBUJ73H\\Poeplau et al. - 2011 - Temporal dynamics of soil organic carbon after lan.pdf:application/pdf;Snapshot:C\:\\Users\\bfrank70\\Zotero\\storage\\DF3D4U2T\\j.1365-2486.2011.02408.html:text/html}
}

