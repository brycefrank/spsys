	file = {Full Text PDF:C\:\\Users\\bfrank70\\Zotero\\storage\\PYJVBSHJ\\Hopkinson et al. - 2016 - Multisensor and Multispectral LiDAR Characterizati.pdf:application/pdf;Snapshot:C\:\\Users\\bfrank70\\Zotero\\storage\\TJ2X7VY2\\07038992.2016.html:text/html}
}

@article{fedrigo_predicting_2018,
	title = {Predicting temperate forest stand types using only structural profiles from discrete return airborne lidar},
	volume = {136},
	issn = {0924-2716},
	url = {http://www.sciencedirect.com/science/article/pii/S0924271617303623},
	doi = {10.1016/j.isprsjprs.2017.11.018},
	abstract = {Light detection and ranging (lidar) data have been increasingly used for forest classification due to its ability to penetrate the forest canopy and provide detail about the structure of the lower strata. In this study we demonstrate forest classification approaches using airborne lidar data as inputs to random forest and linear unmixing classification algorithms. Our results demonstrated that both random forest and linear unmixing models identified a distribution of rainforest and eucalypt stands that was comparable to existing ecological vegetation class (EVC) maps based primarily on manual interpretation of high resolution aerial imagery. Rainforest stands were also identified in the region that have not previously been identified in the EVC maps. The transition between stand types was better characterised by the random forest modelling approach. In contrast, the linear unmixing model placed greater emphasis on field plots selected as endmembers which may not have captured the variability in stand structure within a single stand type. The random forest model had the highest overall accuracy (84\%) and Cohen’s kappa coefficient (0.62). However, the classification accuracy was only marginally better than linear unmixing. The random forest model was applied to a region in the Central Highlands of south-eastern Australia to produce maps of stand type probability, including areas of transition (the ‘ecotone’) between rainforest and eucalypt forest. The resulting map provided a detailed delineation of forest classes, which specifically recognised the coalescing of stand types at the landscape scale. This represents a key step towards mapping the structural and spatial complexity of these ecosystems, which is important for both their management and conservation.},
	urldate = {2019-06-28},
	journal = {ISPRS Journal of Photogrammetry and Remote Sensing},
	author = {Fedrigo, Melissa and Newnham, Glenn J. and Coops, Nicholas C. and Culvenor, Darius S. and Bolton, Douglas K. and Nitschke, Craig R.},
	month = feb,
	year = {2018},
	keywords = {Random forest, Lidar, Linear unmixing, Principal components, Structure},
	pages = {106--119},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\bfrank70\\Zotero\\storage\\225JIEUW\\Fedrigo et al. - 2018 - Predicting temperate forest stand types using only.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\bfrank70\\Zotero\\storage\\SFAHD4KN\\S0924271617303623.html:text/html}
}

