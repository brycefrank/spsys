@article{shi_important_2018,
	title = {Important {LiDAR} metrics for discriminating forest tree species in {Central} {Europe}},
	volume = {137},
	issn = {0924-2716},
	url = {http://www.sciencedirect.com/science/article/pii/S0924271618300315},
	doi = {10.1016/j.isprsjprs.2018.02.002},
	abstract = {Numerous airborne LiDAR-derived metrics have been proposed for classifying tree species. Yet an in-depth ecological and biological understanding of the significance of these metrics for tree species mapping remains largely unexplored. In this paper, we evaluated the performance of 37 frequently used LiDAR metrics derived under leaf-on and leaf-off conditions, respectively, for discriminating six different tree species in a natural forest in Germany. We firstly assessed the correlation between these metrics. Then we applied a Random Forest algorithm to classify the tree species and evaluated the importance of the LiDAR metrics. Finally, we identified the most important LiDAR metrics and tested their robustness and transferability. Our results indicated that about 60\% of LiDAR metrics were highly correlated to each other ({\textbar}r{\textbar} {\textgreater} 0.7). There was no statistically significant difference in tree species mapping accuracy between the use of leaf-on and leaf-off LiDAR metrics. However, combining leaf-on and leaf-off LiDAR metrics significantly increased the overall accuracy from 58.2\% (leaf-on) and 62.0\% (leaf-off) to 66.5\% as well as the kappa coefficient from 0.47 (leaf-on) and 0.51 (leaf-off) to 0.58. Radiometric features, especially intensity related metrics, provided more consistent and significant contributions than geometric features for tree species discrimination. Specifically, the mean intensity of first-or-single returns as well as the mean value of echo width were identified as the most robust LiDAR metrics for tree species discrimination. These results indicate that metrics derived from airborne LiDAR data, especially radiometric metrics, can aid in discriminating tree species in a mixed temperate forest, and represent candidate metrics for tree species classification and monitoring in Central Europe.},
	urldate = {2018-08-08},
	journal = {ISPRS Journal of Photogrammetry and Remote Sensing},
	author = {Shi, Yifang and Wang, Tiejun and Skidmore, Andrew K. and Heurich, Marco},
	month = mar,
	year = {2018},
	keywords = {Airborne LiDAR, Leaf-off, Leaf-on, Morphology, Natural forest, Tree species},
	pages = {163--174},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\bfrank70\\Zotero\\storage\\JDPDG4A4\\Shi et al. - 2018 - Important LiDAR metrics for discriminating forest .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\bfrank70\\Zotero\\storage\\3L6T56PG\\S0924271618300315.html:text/html}
}

