	keywords = {parameter estimation, anisotropy, GSI, precipitation, rainband},
	pages = {5906--5925},
	file = {Full Text PDF:C\:\\Users\\bfrank70\\Zotero\\storage\\GKI7L4VV\\Niemi et al. - 2014 - A simple and effective method for quantifying spat.pdf:application/pdf}
