@article{packalen_k-msn_2007,
	title = {The k-{MSN} method for the prediction of species-specific stand attributes using airborne laser scanning and aerial photographs},
	volume = {109},
	issn = {0034-4257},
	url = {http://www.sciencedirect.com/science/article/pii/S0034425707000387},
	doi = {10.1016/j.rse.2007.01.005},
	abstract = {Various studies have been presented within the last 10 years on the possibilities for predicting forest variables such as stand volume and mean height by means of airborne laser scanning (ALS) data. These have usually considered tree stock as a whole, even though it is tree species-specific forest information that is of primary interest in Finland, for example. We will therefore concentrate here on prediction of the species-specific forest variables volume, stem number, basal area, basal area median diameter and tree height, applying the non-parametric k-MSN method to a combination of ALS data and aerial photographs in order to predict these stand attributes simultaneously for Scots pine, Norway spruce and deciduous trees as well as total characteristics as sums of the species-specific estimates. The predictor variables derived from the ALS data were based on the height distribution of vegetation hits, whereas spectral values and texture features were employed in the case of the aerial photographs. The data covered 463 sample plots in 67 stands in eastern Finland, and the results showed that this approach can be used to predict species-specific forest variables at least as accurately as from the current stand-level field inventory for Finland. The characteristics of Scots pine and Norway spruce were predicted more accurately than those of deciduous trees.},
	number = {3},
	urldate = {2018-06-22},
	journal = {Remote Sensing of Environment},
	author = {Packalén, Petteri and Maltamo, Matti},
	month = aug,
	year = {2007},
	keywords = {Airborne laser scanning, Aerial photographs, Species-specific stand attributes},
	pages = {328--341},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\bfrank70\\Zotero\\storage\\92IKQ8QT\\Packalén and Maltamo - 2007 - The k-MSN method for the prediction of species-spe.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\bfrank70\\Zotero\\storage\\72HNCTUH\\S0034425707000387.html:text/html}
}

@article{lemay_comparison_2005,
	title = {Comparison of {Nearest} {Neighbor} {Methods} for {Estimating} {Basal} {Area} and {Stems} per {Hectare} {Using} {Aerial} {Auxiliary} {Variables}},
	volume = {51},
	abstract = {Simulations were used to compare variable-space nearest neighbor methods for imputing stems per ha and basal area per ha (ground measured) for complex stands (many species and sizes) of southeastern British Columbia, Canada. Species composition and other characteristics obtained for every stand by interpreting aerial photography were used as auxiliary variables. The simulations included three distance metrics (squared Euclidean distance, most similar neighbor, and absolute distance), three intensities of stands with full information (20\%, 50\%, and 80\%), two sets of aerial variables (mixed versus moderately high correlations with ground variables), and three averaging methods (nearest neighbor, average of three nearest neighbors, and distance weighted average of three nearest neighbors weighted). Increasing the number of stands with full information to 50\% from 20\% resulted in increased accuracy, with no noticeable improvement with a further increase to 80\%. Of the three distance metrics, the most similar neighbor measure gave good results in imputing stems per ha and basal area per ha, particularly when there was a mixture of correlations, high and moderate, between the auxiliary (aerial) variables, and the ground variables. No large gain was noted in using the average of three neighbors rather than a single neighbor.},
	language = {en},
	number = {2},
	urldate = {2018-06-22},
	journal = {Forest Science},
	author = {LeMay, Valerie and Temesgen, Hailemariam},
